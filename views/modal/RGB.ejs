<div id="<%- ID %>" class="card" style="display: none;">
	<header class="card-header">
		<p class="card-header-title">
			<%- Name %>
		</p>
		<button class="card-header-icon" aria-label="more options">
			<span class="icon">
				<i class="fas fa-angle-down" aria-hidden="true"></i>
			</span>
		</button>
	</header>
	<div class="card-content">
		<div class="content">
			<div class="box">
				<div id="<%- ID %>Color"></div>
				<br>
				<div class="field has-addons">
					<p class="control">
						<input class="input" id="<%- ID %>Value" type="text" value="255, 255, 255" readonly>
					</p>
				</div>
			</div>
		</div>
	</div>
</div>
<script>
			// pin: [< LED.pin %>],
			// color: [RGB.r,RGB.g,RGB.b]
	var <%- ID %> = new iro.ColorPicker('#<%- ID %>Color', {
		layout: [{
			component: iro.ui.Wheel,
		}, {
			component: iro.ui.Slider,
			options: {
				sliderType: 'red',
			}
		}, {
			component: iro.ui.Slider,
			options: {
				sliderType: 'green',
			}
		}, {
			component: iro.ui.Slider,
			options: {
				sliderType: 'blue',
			}
		}, {
			component: iro.ui.Slider,
			options: {
				sliderType: 'value',
			}
		}]
	});
	<%- ID %>.on('color:change', <%- ID %>SetRGB);
	function <%- ID %>SetRGB(RGB) {
		RGB.ID = "<%- ID %>Value";
		SetRGB(RGB);
	}
</script>